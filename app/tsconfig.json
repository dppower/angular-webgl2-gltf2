{
    "compilerOptions": {
        "target": "es5",
        "lib": [ "es2015", "dom", "dom.iterable" ],
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../build/app"
    }
}